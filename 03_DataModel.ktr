    <connection>localhost</connection>
    <sql>SELECT
    r.*,
    TO_CHAR(r.creation_date,'MON') as MONTH,
    TO_CHAR(r.creation_date,'YYYY') as YEAR,
    TO_CHAR(r.creation_date,'MON')||'-'||TO_CHAR(r.creation_date,'YYYY') as MONTH_YEAR
FROM 
    (SELECT
        (select country from country c where c.id=rd.country_id) as COUNTRY,
        (select category from CATEGORY c where c.id=rd.category_id) as CATEGORY,
        (select status from ORDER_STATUS c where c.status_code=rd.status_id) as STATUS,
        CASE
            WHEN length(rd.creation_date) = 5 THEN 
                to_date('0'||substr(rd.creation_date,0,2) || substr(rd.creation_date,3,2) || substr(rd.creation_date,5,2),'DDMMYY')
            WHEN length(rd.creation_date) = 6 THEN
                to_date(substr(rd.creation_date,0,2) || substr(rd.creation_date,3,2) || substr(rd.creation_date,5,2),'DDMMYY')
            ELSE NULL
        END AS creation_date,
        rd.order_id,
        rd.order_value
    FROM rd_ORDER rd ) r</sql>
